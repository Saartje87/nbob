{
	"project": {
		"name": "Unnamed",
		"version": "0",
		"files": [ "**/*", "!build/**/*", "!dist/**/*", "!nbob-config.json", "!*.sublime-*" ]
	},
	"analyze:js:hint": {
		"files": [ "**/*.js" ],
		"rcFile": ".jshintrc",
		"ignoreFile": ".jshintignore"
	},
	"build:html:l10n": {
		"locale": "en-us",
		"files": [ "**/*-l10n.html", "**/*-l10n/**/*.html" ]
	},
	"build:js:concat": {
		"files": [ "lib/**/*.js", "src/**/*.js" ]
	},
	"deploy": {
		"user": null,
		"accessKey": null,
		"secretKey": null
	}
}
