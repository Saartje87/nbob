{
	"project": {
		"name": "Unnamed",
		"version": "0",
		"files": [
			"**/*",
			"!build/**/*",
			"!dist/**/*",
			"!nbob-config.json",
			"!*.sublime-*",
			"!node_modules/**/*"
		],
		"buildDir": "build",
		"distDir": "dist"
	},
	"analyze:js:hint": {
		"files": [ "**/*.js" ],
		"rcFile": ".jshintrc",
		"ignoreFile": ".jshintignore"
	},
	"update:l10n": {
		"locales": "l10n/*.json",
		"l10nFiles": "**/*-l10n.html",
		"l10nDirs": "**/*-l10n/**/*.html",
		"files": [ "{{update:l10n.locales}}", "{{update:l10n.l10nFiles}}", "{{update:l10n.l10nDirs}}" ]
	},
	"build:l10n": {
		"locales": "{{update:l10n.locales}}",
		"l10nFiles": "{{update:l10n.l10nFiles}}",
		"l10nDirs": "{{update:l10n.l10nDirs}}",
		"files": [ "{{build:l10n.locales}}", "{{build:l10n.l10nFiles}}", "{{build:l10n.l10nDirs}}" ],
		"default": null
	},
	"build:html:templates": {
		"files": [ "templates/*/*.html" ],
		"ext": ".html"
	},
	"build:css:less": {
		"files": [ "**/*.less" ]
	},
	"build:js:minify": {
		"files": [ "{lib,src}/**/*.js{,.map}", "!**/*.min.js" ]
	},
	"build:js:concat": {
		"files": [ "{lib,src}/**/*.min.js{,.map}" ],
		"output": "{{project.name}}-{{project.version}}.min.js"
	},
	"build:js:amd": {
		"files": [ "{{build:js:concat.output}}{,.map}" ],
		"exports": []
	},
	"build:dist": {
		"files": [ "**/*", "!**/*.less" ]
	},
	"server": {
		"port": 8080
	},
	"deploy": {
		"files": [ "**/*", "!**/*.{gv,less,map}" ],
		"region": "eu-west-1",
		"bucketName": null,
		"force": false,
		"cacheControl": "max-age=3600",
		"gzipThreshold": 1000
	},
	"envConfigMap": {}
}
